{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug onnx_demo",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/onnx_demo",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [{ "description": "Enable pretty-printing", "text": "-enable-pretty-printing", "ignoreFailures": true }]
    }
  ]
}
